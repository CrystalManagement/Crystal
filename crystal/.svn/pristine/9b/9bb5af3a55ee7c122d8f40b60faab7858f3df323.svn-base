DROP TABLE IF EXISTS `consultant`;
CREATE TABLE `consultant` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userid` int(11) DEFAULT NULL,
  `name` varchar(8) DEFAULT NULL,
  `gender` tinyint(1) DEFAULT NULL COMMENT '性别',
  `birthday` date DEFAULT NULL,
  `certificate_no` varchar(20) DEFAULT NULL COMMENT '证件号',
  `certificate_type` tinyint(4) DEFAULT NULL COMMENT '证件类型',
  `certificate_pic1` varchar(1000) DEFAULT NULL,
  `certificate_pic2` varchar(1000) DEFAULT NULL,
  `tel` varchar(20) DEFAULT NULL,
  `city` int(11) DEFAULT NULL,
  `photo` varchar(100) DEFAULT NULL,
  `working_years` tinyint(4) DEFAULT NULL,
  `personal_analysis_time` int(11) DEFAULT NULL,
  `supervised_time` int(11) DEFAULT NULL,
  `style` varchar(511) DEFAULT NULL,
  `introduce` varchar(511) DEFAULT NULL,
  `training` varchar(2047) DEFAULT NULL COMMENT '培训经历',
  `education` varchar(2047) DEFAULT NULL COMMENT '教育经历',
  `work_experience` varchar(2047) DEFAULT NULL COMMENT '工作经历',
  `supervisory_experience` varchar(2047) DEFAULT NULL COMMENT '督导经历',
  `cases` varchar(2047) DEFAULT NULL COMMENT '案例',
  `expert_area` varchar(100) DEFAULT NULL COMMENT '擅长领域',
  `oriented_crowd` varchar(100) DEFAULT NULL COMMENT '面向人群',
  `auth_state` tinyint(4) DEFAULT NULL COMMENT '审核状态',
  `interview_price` smallint(6) DEFAULT NULL,
  `video_price` smallint(6) DEFAULT NULL,
  `tel_price` smallint(6) DEFAULT NULL,
  `price` smallint(6) DEFAULT NULL,
  `service_time` int(11) DEFAULT NULL,
  `average_score` tinyint(4) DEFAULT NULL COMMENT '平均评分',
  `order_score` int(11) DEFAULT NULL COMMENT '排序字段',
  `visible` tinyint(1) DEFAULT '1',
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`),
  UNIQUE KEY `AK_Key_2` (`userid`),
  KEY `AK_Key_3` (`city`),
  KEY `AK_Key_4` (`price`),
  KEY `AK_Key_5` (`service_time`),
  KEY `AK_Key_6` (`average_score`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

